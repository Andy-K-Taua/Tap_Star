<!-- Photos -->

<h2>New Photos</h2>

<%= form_with model: @photo, local: true do |f| %>

  <%= f.label :image %><br>
  <%= f.text_field :image %><br><br>

  <%= f.label :rating %><br>
  <%= f.text_field :rating %><br><br>

  <%= f.hidden_field :place_id, value: @place_id %>

  <%= f.submit %>

<% end %>
